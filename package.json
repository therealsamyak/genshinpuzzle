{
  "name": "genshinpuzzle",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "rebuild": "rm -rf node_modules && rm -rf dist && deno install && deno task prepare && deno task build",
    "lint": "oxlint --fix .",
    "format": "oxfmt .",
    "check": "deno task format && deno task lint",
    "preview": "vite preview",
    "prepare": "lefthook install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "generate:elements": "deno run -A scripts/generate_character_elements.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.21",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "gh-pages": "^6.3.0",
    "globals": "^17.3.0",
    "lefthook": "^2.1.0",
    "oxfmt": "^0.31.0",
    "oxlint": "^1.46.0",
    "supabase": "^2.76.8",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  }
}
